package MisClases;

public class arrayEjemplo {

	public static void main(String[] srgs) {
		
		//DECLARAMOS UNA VARIABLE DE TIPOM ARRAY A ENTEROS 
		//OJO EL ARRAY TODAVIA NO ESTA CREADO
		int[] arNumeros;
		//CREAMOS EL ARRAY Y SE LO ASIGNAMOS A LA VARIABLE ANTERIOR
		arNumeros=new int[10] ;
		
		arNumeros= {};
		//int[] arNumeros1=new int[10] ;
		
		//NOS RECORREMOS LAS DISTINTAS POSICIONES DEL ARRAY 
		for(int i=0; i < arNumeros.length; i++) {
			int valor=arNumeros[i];
			System.out.println();
			
		}
	}
}
